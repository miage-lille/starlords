{
    "$schema": "https://json.schemastore.org/tsconfig",
    "display": "Node 16",

    "compilerOptions": {
      "lib": ["ESNext"],
      "module": "CommonJS",
      "target": "ESNext",
      "resolveJsonModule": true,
      "noImplicitAny": false,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "moduleResolution": "Node"
    }
  }